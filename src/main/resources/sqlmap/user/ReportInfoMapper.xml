<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="reportInfo">

    <!--添加-->
    <insert id="insertReportInfo" useGeneratedKeys="true" keyProperty="id" >
      INSERT INTO reportInfo(accountID,content,reportType,reportObject,isValid,createTime) values(#{accountID},#{content},#{reportType},#{reportObject},#{isValid},#{createTime});
    </insert>
</mapper>